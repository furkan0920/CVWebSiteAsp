@model CVSite.Models.Education
@{
	ViewData["Title"] = "EucationGet";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Eudcation</h1>
<br />
@using (Html.BeginForm("EducationUpdate", "Admin", FormMethod.Post))
{
	@Html.LabelFor(x => x.ID)
	@Html.TextBoxFor(x => x.ID, new { @class = "form-control", @readonly = "readonly" })
	<br />
	@Html.LabelFor(x => x.Title)
	@Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
	<br />
	@Html.LabelFor(x => x.SubTitle)
	@Html.TextBoxFor(x => x.SubTitle, new { @class = "form-control" })
	<br />
	@Html.LabelFor(x => x.GNote)
	@Html.TextBoxFor(x => x.GNote, new { @class = "form-control" })
	<br />
	@Html.LabelFor(x => x.Date)
	@Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
	<br />
	<button class="btn btn-success">Update</button>
}
