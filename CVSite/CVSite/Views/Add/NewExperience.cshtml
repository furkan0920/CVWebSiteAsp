@model CVSite.Models.Experience
@{
    ViewData["Title"] = "NewExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>NewExperience</h1>

<br />
<form class="form-group" method="post">

    @Html.LabelFor(x => x.Title)
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.SubTitle)
    @Html.TextBoxFor(x => x.SubTitle, new { @class = "form-control" })
    <br />
  
    @Html.LabelFor(x => x.Description)
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Date)
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Experience Add</button>
</form>