@model CVSite.Models.Education
@{
	ViewData["Title"] = "NewEducation";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>NewEducation</h1>
<br />
<form class="form-group" method="post">

	@Html.LabelFor(x => x.Title)
	@Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
	<br />
	@Html.LabelFor(x => x.SubTitle)
	@Html.TextBoxFor(x => x.SubTitle, new { @class = "form-control" })
	<br />
	@Html.LabelFor(x => x.GNote)
	@Html.TextBoxFor(x => x.GNote, new { @class = "form-control" })
	<br />
	@Html.LabelFor(x => x.Date)
	@Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
	<br />
	<button class="btn btn-primary">Education Add</button>
</form>

